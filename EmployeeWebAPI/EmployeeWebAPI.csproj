    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>bf259ac1-2b14-443d-9dd1-e58ae27dfb0a</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.0-preview.4.23259.3" />
